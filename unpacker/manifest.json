{
  "name": "__MSG_name__",
  "version": "0.73",
  "manifest_version": 2,
  "minimum_chrome_version": "44.0.2400.0",
  "description": "__MSG_description__",
  "default_locale": "en",
  "display_in_launcher": false,
  "permissions": [
    "fileSystemProvider",
    {"fileSystem": ["retainEntries"]},
    "notifications",
    "storage",
    "alwaysOnTopWindows"
  ],
  "file_system_provider_capabilities": {
    "multipleMounts": true,
    "source": "file"
  },
  "file_handlers": {
    "zip": {
      "types": ["application/zip"],
      "extensions": ["zip"]
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/app.js",
        "js/background.js",
        "js/decompressor.js",
        "js/passphrase-manager.js",
        "js/request.js",
        "js/volume.js"
      ]
    }
  }
}
